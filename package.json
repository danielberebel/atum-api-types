{
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "np --no-tests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "atum-api-types",
  "author": "Daniel BeRebel",
  "module": "dist/atum-api-types.esm.js",
  "size-limit": [
    {
      "path": "dist/atum-api-types.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/atum-api-types.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "np": "^7.6.1",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "wc-api-types": "^1.1.1",
    "wp-api-types": "^1.0.0"
  }
}
